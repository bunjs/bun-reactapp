{
    presets: [
        ['env', {
            "targets": {
                "browsers": ["last 2 versions", "safari >= 7"]
            },
            "modules": 'commonjs', //禁用babel-preset-env的模块化规范转化
            "debug": false,
            "useBuiltIns": true // 是否开启自动支持 polyfill
            // 'stage-2'
        }]
    ],
    "plugins": [
        ['transform-react-jsx'], 
        ["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": "css" }]
    ]
}